{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug System Planner",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": ["--extensionDevelopmentPath=${workspaceRoot}/packages/ide"],
			"outFiles": ["${workspaceFolder}/packages/ide/out/**/*.js"],
			"preLaunchTask": "system-planner: build"
		},
		{
			"name": "Launch IDE Extension (Production)",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": ["--extensionDevelopmentPath=${workspaceRoot}/packages/ide"],
			"outFiles": ["${workspaceFolder}/packages/ide/out/**/*.js"],
			"preLaunchTask": "ide: build"
		},
		{
			"name": "Debug ELF file explorer webview",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": ["--extensionDevelopmentPath=${workspaceRoot}/packages/ide"],
			"outFiles": ["${workspaceFolder}/packages/ide/out/**/*.js"],
			"preLaunchTask": "elf: build"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Execute CLI Dev Command",
			"skipFiles": ["<node_internals>/**"],
			"program": "${workspaceFolder}/packages/cli/bin/dev.js",
			"args": ["topic", "--someFlag"],
			"env": {
				"CFSUTIL_CONFIG_HOME": "${workspaceFolder}/packages/cli/config",
				"XDG_CONFIG_HOME": "${workspaceFolder}/packages/cli/config"
			},
			"runtimeExecutable": "${workspaceRoot}/node_modules/.bin/tsx",
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen"
		}
	]
}
