{
  "name": "cfs-ccm-lib",
  "description": "Client library for CodeFusion Studio's Cloud Catalog Management Service",
  "version": "2.0.0",
  "engines": {
    "node": ">=20"
  },
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "/dist",
    "!tsconfig.tsbuildinfo"
  ],
  "devDependencies": {
    "@tsconfig/node20": "^20.1.6",
    "@types/chai": "^5.2.2",
    "@types/chai-as-promised": "^8.0.2",
    "@types/express": "^5.0.1",
    "@types/lodash": "^4.17.20",
    "@types/lowdb": "^1.0.15",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.19.11",
    "@types/semver": "^7.5.8",
    "@types/temp": "^0.9.4",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "c8": "^10.1.3",
    "chai": "^5.3.1",
    "chai-as-promised": "^8.0.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-jsdoc": "^50.8.0",
    "mocha": "^11.7.1",
    "nock": "^14.0.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "shx": "^0.4.0",
    "sinon": "^21.0.0",
    "tsx": "^4.20.4",
    "typedoc": "0.28.10",
    "typedoc-plugin-markdown": "^4.8.1",
    "typedoc-plugin-missing-exports": "^4.1.0",
    "typedoc-plugin-rename-defaults": "^0.7.3",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "env-paths": "^3.0.0",
    "express": "^5.1.0",
    "jszip": "^3.10.1",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "lowdb": "^7.0.1",
    "node-fetch-cache": "^5.0.2",
    "open": "^10.2.0",
    "openapi-fetch": "^0.14.0",
    "openid-client": "^6.6.4",
    "semver": "^7.7.2",
    "temp": "^0.9.4",
    "zod": "^3.25.76"
  },
  "scripts": {
    "build": "tsc -b && typedoc",
    "clean": "shx rm -rf dist \"./**tsconfig.tsbuildinfo\"",
    "format": "npm run format:cmd -- \"**/*.{ts,js,cjs,mjs,json}\"",
    "format:cmd": "prettier --check",
    "format:fix": "npm run format -- --write",
    "generate:docs": "typedoc",
    "lint": "npm run lint:cmd -- .",
    "lint:cmd": "eslint --ext .ts,.js,.cjs,.mjs",
    "lint:fix": "npm run lint -- --fix",
    "prepack": "run-s clean build",
    "pretest": "tsc --noEmit",
    "test": "c8 mocha \"test/**/*.test.ts\" --reporter-option output=coverage/test-results.json"
  }
}
