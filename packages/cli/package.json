{
  "author": "Analog Devices, Inc.",
  "bin": "./bin/run.js",
  "description": "ADI CodeFusion Studio CLI tool",
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^4",
    "cfs-lib": "workspace:*",
    "cfs-package-manager": "workspace:*",
    "cli-table3": "^0.6.5",
    "elf-parser": "workspace:*",
    "env-paths": "^3.0.0"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/copyfiles": "^2",
    "@types/mocha": "^10",
    "@types/node": "^20",
    "chai": "^4",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.5.0",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "3.0.38",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "mocha": "^10",
    "oclif": "^4.1.3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": "^20.x"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/analogdevicesinc/codefusion-studio/tree/V2.0.0/packages/cli",
  "license": "MIT",
  "main": "",
  "name": "cfsutil",
  "oclif": {
    "bin": "cfsutil",
    "dirname": "cfsutil",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "elf": {
        "description": "ELF parsing library."
      },
      "dt": {
        "description": "Devicetree parser."
      },
      "cfsplugins": {
        "description": "CFS plugins."
      },
      "workspace": {
        "description": "Workspace."
      },
      "project": {
        "description": "Project."
      },
      "pkg": {
        "description": "CFS Package Manager."
      },
      "auth": {
        "description": "CFS backend authentication."
      }
    }
  },
  "repository": "https://github.com/analogdevicesinc/codefusion-studio/tree/V2.0.0/packages/cli",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b && copyfiles -u 1 'src/config/*.json' dist",
    "build-local-deps": "yarn workspaces foreach -Rpt --topological-dev --exclude cfs-plugins-api run build",
    "auth-cfg": "node build-scripts/auth-config.js > src/config/auth.json",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn auth-cfg && yarn build && oclif manifest && oclif readme",
    "prepare": "yarn build",
    "tarballs": "yarn build-local-deps && node build-scripts/tarballs.js && oclif pack tarballs \"$@\" && shx mv -f package.json.bak package.json",
    "test": "CFS_INSTALL_DIR=\"test/fixtures\" c8 mocha --forbid-only \"test/commands/**/*.test.ts\" --reporter-option output=coverage/test-results.json",
    "test-plugins": "CFS_INSTALL_DIR=\"test/fixtures\" mocha --forbid-only \"test/plugins/**/*.test.ts\"",
    "test-gen": "CFS_INSTALL_DIR=\"test/fixtures\" mocha --forbid-only \"test/commands/generate.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "version": "2.0.0",
  "bugs": "https://github.com/analogdevicesinc/codefusion-studio/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "exports": "./lib/index.js",
  "type": "module"
}
